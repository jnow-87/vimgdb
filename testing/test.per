property little-endian

range 0x612c20 10{
	section "first"{
		//			<name>	<offset>	<nbytes>
		register "r1" "Register 1" 0 4{
			//		<name>	<index>	<nbits>
			bits "30/31" 28 4
			bits "0/1" 0 8
		}

		emptyline
		heading "this is a heading"
